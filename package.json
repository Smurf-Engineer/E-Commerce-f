{
  "name": "jackroo-web",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "generate": "plop --plopfile ./node_modules/react-components-generator/index.js",
    "test:dev": "razzle test --env=jsdom",
    "test": "jest --config ./jest.config.js",
    "start:prod": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@types/express": "^4.11.0",
    "@types/graphql": "^0.12.3",
    "@types/jest": "^22.1.0",
    "@types/lodash": "^4.14.103",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-swipeable-views": "^0.12.0",
    "@types/redux-logger": "^3.0.5",
    "@types/serialize-javascript": "^1.3.2",
    "@types/webpack-env": "^1.13.3",
    "@types/zenscroll": "^4.0.0",
    "antd": "^3.1.6",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.1",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "babel-plugin-styled-components": "^1.5.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "immutable": "^3.8.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.5",
    "node-fetch": "1.6",
    "node-noop": "^1.0.0",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "razzle": "^0.8.7",
    "react": "^16.2.0",
    "react-animate-height": "^0.10.9",
    "react-apollo": "^2.0.4",
    "react-components-generator": "^0.0.4",
    "react-dom": "^16.2.0",
    "react-facebook-login": "^4.0.0",
    "react-google-login": "^3.0.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-star-ratings": "^2.2.0",
    "react-swipeable-views": "^0.12.12",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "styled-components": "^3.1.3",
    "ts-jest": "^22.0.2",
    "ts-loader": "^3.3.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "zenscroll": "^4.0.0"
  },
  "devDependencies": {},
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "^.+\\.svg?$": "<rootDir>/__mocks__/svgTransform.js"
    }
  }
}
